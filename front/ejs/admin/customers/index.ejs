<% include ../parts/_header %>
<div class="l-admin__right__foot__content__full">
    <div class="c-buttonList">
        <div class="c-buttonList__left"></div>
        <div class="c-buttonList__right">
            <p class="c-buttonList__right__list"><a href="">CSV出力</a></p>
            <p class="c-buttonList__right__list">CSVインポート</p>
            <p class="c-buttonList__right__list"><a href="/admin/settings/users/new">新規登録</a></p>
        </div>
    </div>
    <table class="c-table">
        <thread class="c-table__thread">
            <tr class="c-table__thread__row">
                <th class="c-table__thread__row__head">ID</th>
                <th class="c-table__thread__row__head">名前</th>
                <th class="c-table__thread__row__head">メールアドレス</th>
                <th class="c-table__thread__row__head">削除</th>
            </tr>
        </thread>
        <tbody class="c-table__body">
            <tr class="c-table__body__row" ng-repeat="user in data.admin_users">
                <td class="c-table__body__row__data">{{user.id}}</td>
                <td class="c-table__body__row__data">{{user.name}}</td>
                <td class="c-table__body__row__data">{{user.email}}</td>
                <td class="c-table__body__row__data">
                    <div class="c-button--detail">
                        <p class="c-button--detail__text"><a href="/admin/settings/users/{{user.id}}">削除する</a></p>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
</div>
<% include ../parts/_footer %>