
<ul class="p-categoryList u-mt-40">
  <li>
    <div class="categoryWrapper">
      <p>色の変更</p>
      <%= form_for([:admins, @color] , :url => {:action => 'update'}) do |f| %>
      <p>ID:<%= @color.id %></p>
      <%= f.text_field :id , class: 'form-control' %>
      <p>種類:<%= @color.color %></p>
      <%= f.text_field :color , class: 'form-control' %>

      <%= f.submit :編集する , class: "btn btn-primary"%>
      <% end %>
    </div>
  </li>
</ul>
