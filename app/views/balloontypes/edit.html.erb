<%= render partial: 'header', locals: { admin_user: @admin_user } %>


<ul class="p-categoryList u-mt-40">
  <li>
    <div class="categoryWrapper">
      <%= form_for([:admins, @balloontype] , :url => {:action => 'update'}) do |f| %>
      <p>バルーンタイプの変更</p>
      <p>ID:<%= @balloontype.id %></p>
      <%= f.text_field :id , class: 'form-control' %>
      <p>タイプ名:<%= @balloontype.balloontype %></p>
      <%= f.text_field :balloontype , class: 'form-control' %>

      <%= f.submit :編集する , class: "btn btn-primary"%>
      <% end %>
    </div>
  </li>
</ul>
